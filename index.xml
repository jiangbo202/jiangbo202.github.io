&lt;script&gt;
(function() {
  document.addEventListener(&#39;DOMContentLoaded&#39;, () =&gt; {
    document.querySelectorAll(&#39;.gallery-wrapper&#39;).forEach(gallery =&gt; {
      const container = gallery.querySelector(&#39;.gallery-container&#39;);
      if (!container) return;

      const items = Array.from(container.children);
      if (items.length === 0) return;

      const prevBtn = gallery.querySelector(&#39;.gallery-nav-btn.prev&#39;);
      const nextBtn = gallery.querySelector(&#39;.gallery-nav-btn.next&#39;);

      
      const lightbox = gallery.querySelector(&#39;.lightbox-overlay&#39;);
      const mediaContainer = gallery.querySelector(&#39;.lightbox-media-container&#39;);
      const lightboxImg = gallery.querySelector(&#39;.lightbox-content-img&#39;);
      const lightboxVideo = gallery.querySelector(&#39;.lightbox-content-video&#39;);
      const lightboxClose = gallery.querySelector(&#39;.lightbox-close&#39;);
      const lightboxPrev = gallery.querySelector(&#39;.lightbox-nav.prev&#39;);
      const lightboxNext = gallery.querySelector(&#39;.lightbox-nav.next&#39;);
      const lightboxLiveBadge = gallery.querySelector(&#39;.lightbox-live-badge&#39;);

      let galleryItems = []; 
      let currentItemIndex = -1;

      
      items.forEach(item =&gt; {
        let photoSrc = &#39;&#39;;
        let videoSrc = null;

        const livePhotoElement = item.querySelector(&#39;[data-photo-src]&#39;);
        if (livePhotoElement) {
          photoSrc = livePhotoElement.dataset.photoSrc;
          videoSrc = livePhotoElement.dataset.videoSrc || null;
        } else {
          const imgElement = item.tagName === &#39;IMG&#39; ? item : item.querySelector(&#39;img&#39;);
          if (imgElement) {
            photoSrc = imgElement.src;
          }
        }

        if (photoSrc) {
          galleryItems.push({ photoSrc, videoSrc });
        }
      });

      const showItem = (index) =&gt; {
        if (index &lt; 0 || index &gt;= galleryItems.length) return;
        currentItemIndex = index;

        const item = galleryItems[index];

        lightboxImg.src = item.photoSrc;
        lightboxImg.style.display = &#39;block&#39;;

        if (item.videoSrc) {
            lightboxVideo.src = item.videoSrc;
            lightboxVideo.style.display = &#39;none&#39;;
            if (lightboxLiveBadge) lightboxLiveBadge.style.display = &#39;block&#39;; 
        } else {
            lightboxVideo.src = &#39;&#39;;
            lightboxVideo.style.display = &#39;none&#39;;
            if (lightboxLiveBadge) lightboxLiveBadge.style.display = &#39;none&#39;; 
        }

        lightbox.classList.add(&#39;show&#39;);
        lightboxPrev.disabled = (index === 0);
        lightboxNext.disabled = (index === galleryItems.length - 1);
      };

      items.forEach((item, index) =&gt; {
        item.addEventListener(&#39;click&#39;, () =&gt; {
          let photoSrc = &#39;&#39;;
          const livePhotoElement = item.querySelector(&#39;[data-photo-src]&#39;);
          if (livePhotoElement) {
            photoSrc = livePhotoElement.dataset.photoSrc;
          } else {
            const imgElement = item.tagName === &#39;IMG&#39; ? item : item.querySelector(&#39;img&#39;);
            if (imgElement) {
              photoSrc = imgElement.src;
            }
          }
          const sourceIndex = galleryItems.findIndex(i =&gt; i.photoSrc === photoSrc);
          if(sourceIndex !== -1) {
            showItem(sourceIndex);
          }
        });
      });

      
      mediaContainer.addEventListener(&#39;mouseenter&#39;, () =&gt; {
        const item = galleryItems[currentItemIndex];
        if (item &amp;&amp; item.videoSrc) {
          lightboxImg.style.display = &#39;none&#39;;
          lightboxVideo.style.display = &#39;block&#39;;
          lightboxVideo.play().catch(e =&gt; {}); 
        }
      });

      mediaContainer.addEventListener(&#39;mouseleave&#39;, () =&gt; {
        const item = galleryItems[currentItemIndex];
        if (item &amp;&amp; item.videoSrc) {
          lightboxVideo.pause();
          lightboxVideo.currentTime = 0;
          lightboxImg.style.display = &#39;block&#39;;
          lightboxVideo.style.display = &#39;none&#39;;
        }
      });

      const closeLightbox = () =&gt; {
        lightbox.classList.remove(&#39;show&#39;);
        lightboxVideo.pause();
      };

      lightboxClose.addEventListener(&#39;click&#39;, closeLightbox);
      lightboxPrev.addEventListener(&#39;click&#39;, () =&gt; showItem(currentItemIndex - 1));
      lightboxNext.addEventListener(&#39;click&#39;, () =&gt; showItem(currentItemIndex + 1));

      
      if (items.length &lt;= 1) {
        if (prevBtn) prevBtn.style.display = &#39;none&#39;;
        if (nextBtn) nextBtn.style.display = &#39;none&#39;;
        return;
      }

      prevBtn.addEventListener(&#39;click&#39;, () =&gt; {
        container.scrollBy({ left: -container.clientWidth * 0.85, behavior: &#39;smooth&#39; });
      });

      nextBtn.addEventListener(&#39;click&#39;, () =&gt; {
        container.scrollBy({ left: container.clientWidth * 0.85, behavior: &#39;smooth&#39; });
      });

      const updateControls = () =&gt; {
        const scrollLeft = container.scrollLeft;
        const scrollWidth = container.scrollWidth;
        const clientWidth = container.clientWidth;
        prevBtn.disabled = scrollLeft &lt; 10;
        nextBtn.disabled = scrollLeft + clientWidth &gt;= scrollWidth - 10;
      };

      let scrollTimer;
      container.addEventListener(&#39;scroll&#39;, () =&gt; {
        clearTimeout(scrollTimer);
        scrollTimer = setTimeout(updateControls, 100);
      });

      updateControls();
    });
  });
})();
&lt;/script&gt;&lt;/p&gt;</description>
